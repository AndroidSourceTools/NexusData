{
  "metaVersion": 1,
  "model": {
    "name": "CompanyStructure",
    "version": 1,
    "packageName": "com.github.dkharrat.nexusdata.test",
    "entities": [{
      "name": "Company",
      "attributes": [{
        "name": "name",
        "type": "String",
        "required": true
      }],
      "relationships": [{
        "name": "employees",
        "destinationEntity": "Employee",
        "inverseName": "company",
        "toMany": true
      }]
    }, {
      "name": "Employee",
      "attributes": [{
        "name": "id",
        "type": "Int",
        "required": true
      }, {
        "name": "firstName",
        "type": "String",
        "required": true
      }, {
        "name": "lastName",
        "type": "String"
      }, {
        "name": "hourlyWage",
        "type": "Double",
        "required": true,
        "default": 10.123
      }, {
        "name": "heightInCm",
        "type": "Float"
      }, {
        "name": "active",
        "type": "Bool",
        "required": true,
        "default": true
      }, {
        "name": "dateOfBirth",
        "type": "Date",
        "required": true,
        "default": "1984-02-03T00:00:00"
      }],
      "relationships": [{
        "name": "company",
        "destinationEntity": "Company",
        "inverseName": "employees"
      }, {
        "name": "manager",
        "destinationEntity": "Employee",
        "inverseName": "directReports"
      }, {
        "name": "directReports",
        "destinationEntity": "Employee",
        "inverseName": "manager",
        "toMany": true
      }, {
        "name": "passport",
        "destinationEntity": "Passport",
        "inverseName": "employee"
      }, {
        "name": "address",
        "destinationEntity": "Address",
        "inverseName": "employee"
      }]
    }, {
     "name": "Address",
     "attributes": [{
       "name": "streetName",
       "type": "String"
     }, {
       "name": "country",
       "type": "String",
       "required": true
     }],
      "relationships": [{
        "name": "employee",
        "destinationEntity": "Employee",
        "inverseName": "address"
      }]
    }, {
      "name": "Passport",
      "attributes": [{
        "name": "number",
        "type": "String",
        "required": true
      }, {
        "name": "country",
        "type": "String",
        "required": true
      }, {
        "name": "issueDate",
        "type": "Date"
      }, {
        "name": "expirationDate",
        "type": "Date"
      }],
      "relationships": [{
        "name": "employee",
        "destinationEntity": "Employee",
        "inverseName": "passport"
      }]
   }]
  }
}
